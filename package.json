{
  "name": "eslint-config-kokarn",
  "version": "3.0.2",
  "description": "My eslint config",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node scripts/install.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kokarn/eslint-config-kokarn.git"
  },
  "author": "Kokarn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kokarn/eslint-config-kokarn/issues"
  },
  "homepage": "https://github.com/kokarn/eslint-config-kokarn#readme",
  "dependencies": {
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-react": "^7.1.0"
  },
  "peerDependencies": {
    "eslint": "^5.6.0"
  },
  "eslintConfig": {
    "extends": "./index.js",
    "env": {
      "node": true
    },
    "rules": {
      "array-bracket-newline": [
        "error",
        "always"
      ],
      "no-magic-numbers": [
        "off"
      ]
    }
  }
}
